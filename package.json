  "private": true,
  "scripts": {
    "dev": "cross-env NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "ts-node": "ts-node --compiler-options {\\\"module\\\":\\\"CommonJS\\\"}",
    "create:dummy": "npm run ts-node -- prisma/seedDummy.ts",
    "create:manyDummy": "npm run ts-node -- prisma/seedManyDummy.ts",
    "test-dotenv": "dotenv -e  .env.test",
    "test-db-dbpush": "npm run test-dotenv  -- prisma db push --accept-data-loss",
    "test-db-seed": "npm run test-dotenv -- prisma db seed",
    "setup:test-db": "npm run test-db-dbpush && npm run test-db-seed",
    "test:server": "jest -i -c jest.server.config.js",
    "test:client": "jest -i -c jest.client.config.js"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
